{% extends 'base.html.twig' %}

{% block body %}

    <div class="container">
        <div class="row">
            <div class="col-sm-offset-3 col-sm-5">
                <h1>Création d'une Marque</h1>
            </div>
        </div>
    </div>

    <div style="margin-top: 20px" class="container">
        <div class="row">
            <div class="col-sm-offset-4 col-sm-4">
                {{ form_start(form) }}
                {{ form_widget(form) }}

               <input type="submit" value="Création" />
                {{ form_end(form) }}
            </div>
        </div>
    </div>

    <div style="margin-top: 20px" class="container">
        <div class="row">
            <div class="col-sm-offset-4 col-sm-4">
                <ul>
                    <li>
                        <a href="{{ path('marque_index') }}">Retour à la liste</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>


{% endblock %}
